@article{ ISI:000466961700001,
Author = {Hipsley, Christy A. and Sherratt, Emma},
Title = {{Psychology, not technology, is our biggest challenge to open digital
   morphology data}},
Journal = {{SCIENTIFIC DATA}},
Year = {{2019}},
Volume = {{6}},
Month = {{APR 26}},
Abstract = {{The past two decades have seen a revolution in digital imaging
   techniques for capturing gross morphology, offering an unprecedented
   volume of data for biological research. Despite the rapid increase in
   scientific publications incorporating those images, the underlying
   datasets remain largely inaccessible. As the technical barriers to data
   sharing continue to fall, we face a more intimate, and perhaps more
   complicated, obstacle to open data -the one in our minds.}},
