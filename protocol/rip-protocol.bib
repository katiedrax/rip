@inproceedings{kintsakis2017,
  title = {An Early Introduction to {{STEM}} Education: Teaching {{Computer Programming Principles}} to 5th Graders through an e-Learning Platform {{A}} Game-Based Approach},
  booktitle = {{{PROCEEDINGS OF}} 2017 {{IEEE GLOBAL ENGINEERING EDUCATION CONFERENCE}} ({{EDUCON2017}})},
