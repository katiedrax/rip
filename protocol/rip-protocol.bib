@article{perez-lopez2017,
  title = {``{{THE PROPHECY OF THE CHOSEN ONES}}'': {{AN EXAMPLE OF GAMIFICATION APPLIED TO UNIVERSITY TEACHING}}},
  author = {{Perez-Lopez}, I. J. and Rivera Garcia, E. and Trigueros Cervantes, C.},
