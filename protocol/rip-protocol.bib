  unique-id = {ISI:000501875700001}
}

@article{sayogo2013,
  title = {Exploring the Determinants of Scientific Data Sharing: {{Understanding}} the Motivation to Publish Research Data},
  author = {Sayogo, Djoko Sigit and Pardo, Theresa A.},
  year = {2013},
  month = jan,
  volume = {30},
  pages = {S19-S31},
  issn = {0740-624X},
  doi = {10.1016/j.giq.2012.06.011},
  abstract = {The research community is working to create new capabilities to share data and to deal with issues of data quality, standards, and protection, and ethical and responsible use of shared data. These issues have been found to influence the willingness of researchers to publish data created during the course of their research. We use the results of a survey conducted by the working groups of the DataONE project to present a new-understanding of challenges to the development of global data collections and preservation by systematically examining the determinants of the researchers' likelihood to openly publish research data. This study found two key determinants affecting researchers' willingness to publish their data. First is data management in terms of data management skills and organization support. Second is the acknowledgement of the data set's originator in terms of appreciation and legal and policy requirements. This study also found that the impact of the significant determinants is contingent on the amount of data to be published. Finally, this study calls for further investigation to ascertain the relationship of data management and data quality, and systematic investigation on the roles and responsibility of government within these global data preservations. (C) 2012 Elsevier Inc. All rights reserved.},
  eissn = {1872-9517},
  journal = {GOVERNMENT INFORMATION QUARTERLY},
  keywords = {210430-wos-sharing-determinants},
  number = {1},
  unique-id = {ISI:000315240600003}
}

@article{schober2011,
  title = {Improving Collaboration for Effective Violence Surveillance and Health Promotion},
  author = {Schober, Daniel J. and Ramaswamy, Megha and Choi, Won Sup and Fawcett, Stephen B. and Hindman, Jarrod and Martinez, Jeremy M.},
  year = {2011},
  month = mar,
  volume = {17},
  pages = {174--176},
  issn = {1078-4659},
  doi = {10.1097/PHH.0b013e3181f5424a},
  abstract = {Background: This practice brief report examines violence surveillance efforts among members of the Violence Prevention Advisory Group-a statewide advisory group of scientists and practitioners who inform violence prevention efforts throughout Colorado. Methods: We conducted qualitative interviews with members of the Violence Prevention Advisory Group (N = 19) to explore how violence-related surveillance data are used; we also examined barriers to collaboration. Interview data were analyzed using open coding and consensus building among authors. Results: Surveillance data were used to enable discussions about ``sensitive'' topics, such as race and violence. The development of cross-sector policy recommendations and other types of shared work also emerged from collaborative data use. Proprietary issues such as data ``ownership'' presented barriers to collaboration. Conclusion: Data-sharing protocols, education about laws, and cross-sector planning can improve collaborative data use, enabling more informed violence prevention efforts and potential improvements to the population's health.},
  journal = {JOURNAL OF PUBLIC HEALTH MANAGEMENT AND PRACTICE},
  keywords = {210430-wos-sharing-determinants},
  number = {2},
  unique-id = {ISI:000286975200013}
}

@article{schomakers2020,
  title = {All of Me? {{Users}}' Preferences for Privacy-Preserving Data Markets and the Importance of Anonymity},
  author = {Schomakers, Eva-Maria and Lidynia, Chantal and Ziefle, Martina},
  year = {2020},
  month = sep,
  volume = {30},
  pages = {649--665},
  issn = {1019-6781},
  doi = {10.1007/s12525-020-00404-9},
  abstract = {Privacy-preserving data markets are one approach to restore users' online privacy and informational self-determination and to build reliable data markets for companies and research. We empirically analyze internet users' preferences for privacy in data sharing, combining qualitative and quantitative empirical methods. Study I aimed at uncovering users' mental models of privacy and preferences for data sharing. Study II quantified and confirmed motives, barriers, and conditions for privacy in data markets. Finally, in a conjoint study, trade-offs between decisive attributes that shape the decision to share data are analyzed. Additionally, differences between user groups with high and with low privacy concerns are observed. The results show that the anonymization level has the greatest impact on the willingness to share data, followed by the type of data. Users with higher privacy concerns are less willing to share data in data markets and want more privacy protection. The results contribute to an understanding of how privacy-preserving data markets could be designed to suit users' preferences.},
  early-access-date = {FEB 2020},
  eissn = {1422-8890},
  journal = {ELECTRONIC MARKETS},
  keywords = {210430-wos-sharing-determinants},
  number = {3},
  orcid-numbers = {Schomakers, Eva-Maria/0000-0002-7650-2686},
  unique-id = {ISI:000515871900001}
}

